{
    "name": "nonaje/quill",
    "description": "A simple framework to make lightweight PHP APIs",
    "authors": [
        {
            "name": "Agustin Ferrari"
        }
    ],
    "type": "project",
    "autoload": {
        "psr-4": {
            "Quill\\": "src/"
        },
        "files": ["src/Support/Helpers/GlobalFunctions.php"]
    },
    "require": {
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.0",
        "pestphp/pest": "^2.34"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
